<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<META HTTP-EQUIV="Pragma" CONTENT="no-cache">
@if?!Authenticate@ @AddFile?/grinder/failedlogin.cmvp@ @else@
	@AddRequestParameter?PROCEED=@
	
	@if?!CheckReqParm?PLAYER=@ @if?CheckAuthCode?AUTH_CMDPLAYERS=true@  @AddRequestParameter?PROCEED=TRUE@ @endif@ @endif@
	@if?CheckReqParm?PLAYER=@ @if?CheckAuthCode?ANYMODAREAS=true@ @AddRequestParameter?PROCEED=TRUE@ @endif@ @endif@
	
	@if?CheckReqParm?PROCEED=@
		@AddFile?/grinder/nopriv.cmvp@
	@else@
<html>
<head>
	<!-- tell spiders to sod off -->
	<meta name="ROBOTS" content="NOINDEX, NOFOLLOW">
	<link rel="STYLESHEET" TYPE="TEXT/CSS" HREF="/grinder/grinder.css">
 	<link rel="grinder icon" href="/images/grinder.ico" >
	<title>CoffeeMud MUDGrinder</title>
	<SCRIPT LANGUAGE=JavaScript>
	<!--
		var properTarget='MAP';
		
        function OpenUp(file,named)
        {
            var strFeatures="top=200,left=50,scrollbars=yes,resizeable=no,height=400,width=650,alwaysRaised=yes,dependent=yes";
            w=window.open("/"+file+"?HELPTOPIC="+named,
                        "Help",strFeatures, false);
        }
		function EditBehavior(ME)
		{
			if(ME.selectedIndex==0)
				ReShow();
		}
		function AddBehavior(ME)
		{
			if(ME.selectedIndex>0)
				ReShow();
		}
		function EditAffect(ME)
		{
			if(ME.selectedIndex==0)
				ReShow();
		}
		function AddAffect(ME)
		{
			if(ME.selectedIndex>0)
				ReShow();
		}
		function ChangedClass()
		{
			document.AREAEDIT.CHANGEDCLASS.value="true";
			ReShow();
		}
		function ChangedLevel()
		{
			@if?ItemData?ISARMOR@
				if(!confirm('Have you already chosen material, level, and worn locations?'))
					return;
			@endif@
			@if?ItemData?ISWEAPON@
				if(!confirm('Have you already chosen material, level, weapon class, and weight?'))
					return;
			@endif@
			document.AREAEDIT.CHANGEDLEVEL.value="true";
			ReShow();
		}
		function ReShow()
		{
			document.AREAEDIT.action='/grinder/edititemform.cmvp';
			document.AREAEDIT.target=properTarget;
			document.AREAEDIT.submit();
		}
        function DelItem()
        {
            if(confirm('Confirm: Delete this item?'))
            {
                document.AREAEDIT.DELITEM.value="@RequestParameter?ITEM@";
                document.AREAEDIT.submit();
            }
        }
		
	//-->
	</SCRIPT>
</head>
<BODY>
<CENTER>
@if?CheckReqParm?PLAYER=@
	@if?CheckReqParm?ROOM=ANY@
		<h3>Create/Modify an Item.</H3>
	@else@
		<h3>Modify Item in room '@RoomID@'.</H3>
	@endif@
@else@
	<h3>Modify Item of player '@PlayerID@'.</H3>
@endif@
<P><FONT COLOR=RED><H1>@RequestParameter?ERRMSG@</H1></FONT>
<BR>
@if?CheckReqParm?PLAYER=@
	@if?CheckReqParm?ROOM=ANY@
		<FORM  ACCEPT-CHARSET="UTF-8" NAME=AREAEDIT TARGET=_top METHOD=POST ACTION="/grinder/editnewitemdone.cmvp">
		<INPUT TYPE=HIDDEN NAME=ROOM VALUE="ANY">
		<INPUT TYPE=HIDDEN NAME=MOB VALUE="@RequestParameter?MOB@">
		<SCRIPT LANGUAGE=JavaScript>
		<!--
			properTarget='_top';
		//-->
		</SCRIPT>
	@else@
		<FORM  ACCEPT-CHARSET="UTF-8" NAME=AREAEDIT TARGET=_top METHOD=POST ACTION="/grinder/@if?!CheckReqParm?MOB=@editmobitemdone.cmvp@else@editroomitemdone.cmvp@endif@">
		<INPUT TYPE=HIDDEN NAME=ROOM VALUE="@RoomID@">
		<INPUT TYPE=HIDDEN NAME=MOB VALUE="@RequestParameter?MOB@">
		<INPUT TYPE=HIDDEN NAME=AREA VALUE="@AreaName@">
		<INPUT TYPE=HIDDEN NAME=MAPSTYLE VALUE="@RequestParameter?MAPSTYLE@">
		<INPUT TYPE=HIDDEN NAME=MAPLEVEL VALUE="@RequestParameter?MAPLEVEL@">
	@endif@
@else@
	<FORM  ACCEPT-CHARSET="UTF-8" NAME=AREAEDIT TARGET=_top METHOD=POST ACTION="/grinder/editplayeritemdone.cmvp">
	<INPUT TYPE=HIDDEN NAME=PLAYER VALUE="@PlayerID@">
@endif@
<INPUT TYPE=HIDDEN NAME=AUTH VALUE="@Authenticate?AUTH@">
<INPUT TYPE=HIDDEN NAME=RESPONSEFIELD VALUE="@RequestParameter?RESPONSEFIELD@">
<INPUT TYPE=HIDDEN NAME=REPLACEFIELD VALUE="@RequestParameter?REPLACEFIELD@">
<INPUT TYPE=HIDDEN NAME=DELITEM VALUE="">
<INPUT TYPE=HIDDEN NAME=CHANGEDCLASS VALUE="">
<INPUT TYPE=HIDDEN NAME=CHANGEDLEVEL VALUE="">
<INPUT TYPE=HIDDEN NAME=ITEM VALUE="@RequestParameter?ITEM@">
<INPUT TYPE=HIDDEN NAME=ACTION VALUE="MODIFYITEM">
<TABLE WIDTH=600 BORDER=1 CELLSPACING=0 CELLPADDIN=0>
<TR>
	<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Item Class:</B></FONT></TD>
    <TD WIDTH=450 COLSPAN=2>
    @if?CheckReqParm?<ITEM=CATALOG-@
        <INPUT TYPE=HIDDEN NAME=CLASSES VALUE="@ItemData?CLASSES&CLASSESID@"><FONT COLOR=WHITE>@ItemData?CLASSES&CLASSESID@</FONT>
    @else@
		<SELECT NAME=CLASSES ONCHANGE="ChangedClass();">
	    @if?CheckReqParm?<ITEM=NEWCATA-@
			@ItemData?CLASSES&GENERICONLY@
		@else@
			@ItemData?CLASSES@
		@endif@
		</SELECT>
	@endif@
	</TD>
</TR>
@if?!ItemData?ISGENERIC@
<TR>
	<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Class Level:</B></FONT></TD>
    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=LEVEL SIZE=5 VALUE="@ItemData?LEVEL@">
    </TD>
</TR>
@if?ItemData?ISELECTRONIC@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Magic Level:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=ABILITY SIZE=5 VALUE="@ItemData?ABILITY@">
	    </TD>
	</TR>
@else@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Tech Level:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=ABILITY SIZE=5 VALUE="@ItemData?ABILITY@">
	    </TD>
	</TR>
@endif@
<TR>
	<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Rejuv. Ticks:</B></FONT></TD>
    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=REJUV SIZE=5 VALUE="@ItemData?REJUV@">
    </TD>
</TR>
<TR>
	<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Uses Code:</B></FONT></TD>
    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=USESREMAIN SIZE=5 VALUE="@ItemData?USESREMAIN@">
    </TD>
</TR>
	@if?ItemData?ISARMOR@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Size (0=resizable):</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=HEIGHT SIZE=5 VALUE="@ItemData?HEIGHT@">
	    </TD>
	</TR>
	@endif@
<TR>
	<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Parameters:</B></FONT></TD>
    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=MISCTEXT SIZE=60  MAXLENGTH=255 VALUE="@ItemData?MISCTEXT@"></TD>
</TR>
@else@
@if?ItemData?ISWALLPAPER@
    <TR>
        <TD WIDTH=150><FONT COLOR="#66FFFF"><B>Item Name:</B></FONT></TD>
        @if?CheckReqParm?<ITEM=CATALOG-@
            <TD WIDTH=450 COLSPAN=2><INPUT TYPE=HIDDEN NAME=NAME VALUE="@ItemData?NAME@"><FONT COLOR=WHITE>@ItemData?NAME@</FONT></TD>
        @else@
            @if?CheckReqParm?<ITEM=NEWCATA-@
                <TD WIDTH=450 COLSPAN=2><INPUT TYPE=HIDDEN NAME=NAME VALUE="@ItemData?NAME@"><FONT COLOR=WHITE>@ItemData?NAME@</FONT></TD>
            @else@
                <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=NAME SIZE=60 VALUE="@ItemData?NAME@"></TD>
            @endif@
        @endif@
    </TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Description:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><TEXTAREA ROWS=3 COLS=50 NAME=DESCRIPTION>@ItemData?DESCRIPTION@</TEXTAREA></TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Is Readable:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=CHECKBOX ONCLICK="ReShow();" NAME=ISREADABLE @ItemData?ISREADABLE@></TD>
	</TR>
	@if?CheckReqParm?ISREADABLE=on@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Readable Text:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><TEXTAREA ROWS=3 COLS=60 NAME=READABLETEXT>@ItemData?READABLETEXT@</TEXTAREA></TD>
	</TR>
	@endif@
@else@
    <TR>
        <TD WIDTH=150><FONT COLOR="#66FFFF"><B>Item Name:</B></FONT></TD>
        @if?CheckReqParm?<ITEM=CATALOG-@
            <TD WIDTH=450 COLSPAN=2><INPUT TYPE=HIDDEN NAME=NAME VALUE="@ItemData?NAME@"><FONT COLOR=WHITE>@ItemData?NAME@</FONT></TD>
        @else@
            @if?CheckReqParm?<ITEM=NEWCATA-@
                <TD WIDTH=450 COLSPAN=2><INPUT TYPE=HIDDEN NAME=NAME VALUE="@ItemData?NAME@"><INPUT TYPE=HIDDEN NAME=NEWITEMNAME VALUE="@ItemData?NAME@"><FONT COLOR=WHITE>@ItemData?NAME@</FONT></TD>
            @else@
                <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=NAME SIZE=60 VALUE="@ItemData?NAME@"></TD>
            @endif@
        @endif@
    </TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Displayed:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=DISPLAYTEXT SIZE=60 MAXLENGTH=255 VALUE="@ItemData?DISPLAYTEXT@"></TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Description:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><TEXTAREA ROWS=3 COLS=50 NAME=DESCRIPTION>@ItemData?DESCRIPTION@</TEXTAREA></TD>
	</TR>
	<TR>
		<TD WIDTH=100><FONT COLOR="#66FFFF"><B>GIF filename:</B></FONT></TD>
	    <TD WIDTH=500 COLSPAN=2><INPUT TYPE=TEXT NAME=IMAGE SIZE=50 VALUE="@ItemData?IMAGE@"></TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Class Level:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=LEVEL SIZE=5 VALUE="@ItemData?LEVEL@">
	    @if?ItemData?ISARMOR@
			<INPUT TYPE=BUTTON ONCLICK="ChangedLevel();" NAME=BUTTCHANGEDLEVEL VALUE="ReScore">
		@endif@
	    @if?ItemData?ISWEAPON@
			<INPUT TYPE=BUTTON ONCLICK="ChangedLevel();" NAME=BUTTCHANGEDLEVEL VALUE="ReScore">
		@endif@
	    </TD>
	</TR>
	@if?ItemData?ISRECIPE@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Common Skill:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2>
	    <SELECT NAME=RECIPESKILL>
	    @ItemData?RECIPESKILL@
	    </SELECT>
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Coded Recipe:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2>@ItemData?RECIPEDATA&RECIPEPREFIX=<INPUT TYPE=TEXT NAME=RECIPEDATA### SIZE=60  MAXLENGTH=255 VALUE="&RECIPEPOSTFIX="><BR>&RECIPEFIELDNAME=RECIPEDATA###@</TD>
	</TR>
	@endif@
	@if?!ItemData?ISCOIN@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Base Gold Value:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=VALUE SIZE=5 VALUE="@ItemData?VALUE@">
	    </TD>
	</TR>
	@endif@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Weight:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=WEIGHT SIZE=5 VALUE="@ItemData?WEIGHT@">
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150>
		<FONT COLOR="#66FFFF"><B>@if?ItemData?ISMOBITEM@Available:
		@else@Rejuv. Ticks:@endif@</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=REJUV SIZE=5 VALUE="@ItemData?REJUV@">
	    <I>@if?ItemData?ISMOBITEM@(0=Always,1-100%)@else@(0=Never Rejuv,# ticks)@endif@
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Behaviors:</B></FONT>
		<BR><A target=HELP HREF="/behaviors.cmvp"><FONT COLOR=WHITE><I>help</I></FONT></a></TD>
	    <TD WIDTH=450 COLSPAN=2>
		@ItemData?BEHAVIORS@
		</TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Affects:</B></FONT>
		<BR><A target=HELP href="/affects.cmvp"><FONT COLOR=WHITE><I>help</I></FONT></a></TD>
	    <TD WIDTH=450 COLSPAN=2>
		@ItemData?AFFECTS@
		</TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Item Flags:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><FONT COLOR=WHITE>
			@if?ItemData?ISWEAPON@
			<INPUT TYPE=CHECKBOX NAME=ISTWOHANDED @ItemData?ISTWOHANDED@>Is Two-Handed<BR>
			@endif@
			<INPUT TYPE=CHECKBOX NAME=ISGETTABLE @ItemData?ISGETTABLE@>Is Gettable<BR>
			<INPUT TYPE=CHECKBOX NAME=ISDROPPABLE @ItemData?ISDROPPABLE@>Is Droppable<BR>
			<INPUT TYPE=CHECKBOX NAME=ISREMOVABLE @ItemData?ISREMOVABLE@>Is Removable<BR>
			<INPUT TYPE=CHECKBOX NAME=ISINVISIBLE @ItemData?ISINVISIBLE@>Is Invisible<BR>
			<INPUT TYPE=CHECKBOX NAME=ISHIDDEN @ItemData?ISHIDDEN@>Is Hidden<BR>
			<INPUT TYPE=CHECKBOX NAME=ISSEEN @ItemData?ISSEEN@>Is Unseeable<BR>
			<INPUT TYPE=CHECKBOX NAME=ISBONUS @ItemData?ISBONUS@>Is Magical<BR>
			<INPUT TYPE=CHECKBOX NAME=ISLIGHT @ItemData?ISLIGHT@>Is Glowing (burning)<BR>
			<INPUT TYPE=CHECKBOX NAME=ISEVIL @ItemData?ISEVIL@>Is Evil<BR>
			<INPUT TYPE=CHECKBOX NAME=ISGOOD @ItemData?ISGOOD@>Is Good<BR>
			<INPUT TYPE=CHECKBOX NAME=NONLOCATABLE @ItemData?NONLOCATABLE@>Hidden from Locate Object<BR>
			</FONT>
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Material:</B></FONT>
	    <TD WIDTH=450 COLSPAN=2>
	    <SELECT NAME=MATERIALS>
		@ItemData?MATERIALS@
		</SELECT>
		</TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Secret Identity:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=SECRETIDENTITY SIZE=60  MAXLENGTH=255 VALUE="@ItemData?SECRETIDENTITY@"></TD>
	</TR>
	@if?ItemData?ISAMMO@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Ammunition type:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><FONT COLOR=WHITE>
	    <INPUT TYPE=TEXT NAME=AMMOTYPE SIZE=7 VALUE="@ItemData?AMMOTYPE@">
	    </FONT>
	    </TD>
		<TR>
			<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Ammunition Amt.</B></FONT></TD>
		    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=USESREMAIN SIZE=10 VALUE="@ItemData?USESREMAIN@"></TD>
		</TR>
	</TR>
	@endif@

@if?ItemData?ISDUST@
<TR>
	<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Spells to cast on affected:</B></FONT></TD>
    <TD WIDTH=450 COLSPAN=2>
    @ItemData?READABLESPELLS@
    </TD>
</TR>
@else@
@if?ItemData?ISPERFUME@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF">
		<B>Smell emotes:</B><BR><FONT SIZE=-1><I>(Semicolon Delimited)</I></FONT></FONT>
		</TD>
	    <TD WIDTH=450 COLSPAN=2>
	    <INPUT TYPE=TEXT NAME=SMELLS SIZE=60 VALUE="@ItemData?SMELLS@">
	    </TD>
	</TR>
@else@
@if?ItemData?ISMAP@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Assigned Map Areas:</B></FONT>
		<INPUT TYPE=HIDDEN NAME=ISREADABLE VALUE="on">
		</TD>
	    <TD WIDTH=450 COLSPAN=2>
	    <SELECT NAME=MAPAREAS MULTIPLE>
	    @ItemData?MAPAREAS@
	    </SELECT>
	    </TD>
	</TR>
@else@
@if?ItemData?ISARMOR@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Wearing Data:</B></FONT>
	    <TD WIDTH=450 COLSPAN=2>
	    <SELECT NAME=WORNDATA MULTIPLE>
	    @ItemData?WORNDATA@
	    </SELECT>
	    <BR><FONT COLOR=WHITE>
	    <INPUT TYPE=RADIO NAME=ISTWOHANDED value="on" @ItemData?ISTWOHANDED@>Is worn on All above Locations.
	    <BR>
	    <INPUT TYPE=RADIO NAME=ISTWOHANDED value="" @ItemData?ISUNTWOHANDED@>Is worn on ANY of the above Locations.
	    </FONT>
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Armor Layer:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=LAYER SIZE=5 VALUE="@ItemData?LAYER@">
	    <INPUT TYPE=CHECKBOX NAME=SEETHRU value="on" @ItemData?SEETHRU@>Is see-through.&nbsp;&nbsp;
	    <INPUT TYPE=CHECKBOX NAME=MULTIWEAR value="on" @ItemData?MULTIWEAR@>Is multi-wear.
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Armor Level:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=ARMOR SIZE=5 VALUE="@ItemData?ARMOR@">
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Condition ?/100</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=USESREMAIN SIZE=10 VALUE="@ItemData?USESREMAIN@"></TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Magic Level:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=ABILITY SIZE=5 VALUE="@ItemData?ABILITY@">
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Size (0=resizable):</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=HEIGHT SIZE=5 VALUE="@ItemData?HEIGHT@">
	    <INPUT TYPE=HIDDEN NAME=ISREADABLE VALUE="">
	    <INPUT TYPE=HIDDEN NAME=READABLETEXT VALUE="">
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Only Contents:</B></FONT>
	    <TD WIDTH=450 COLSPAN=2>
	    <SELECT NAME=CONTENTTYPES MULTIPLE>
	    @ItemData?CONTENTTYPES@
	    </SELECT>
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Solid Capacity:</B></FONT></TD>
		<TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=CAPACITY SIZE=5 VALUE="@ItemData?CAPACITY@">
		</TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Has a Lid:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=CHECKBOX NAME=HASALID @ItemData?HASALID@ ONCLICK="ReShow();"></TD>
	</TR>
	@if?CheckReqParm?HASALID=on@
		<TR>
			<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Has a Lock:</B></FONT></TD>
		    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=CHECKBOX NAME=HASALOCK @ItemData?HASALOCK@ ONCLICK="ReShow();"></TD>
		</TR>
		@if?CheckReqParm?HASALOCK=on@
		<TR>
			<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Key Code:</B></FONT></TD>
		    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=KEYCODE SIZE=5 VALUE="@ItemData?KEYCODE@">
		    </TD>
		</TR>
		@endif@
	@endif@
@else@
@if?ItemData?ISFOOD@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Nourishment ?/1000:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=NOURISHMENT SIZE=5 VALUE="@ItemData?NOURISHMENT@">
	    <INPUT TYPE=HIDDEN NAME=ISREADABLE VALUE="">
	    </TD>
	</TR>
    <TR>
        <TD WIDTH=150><FONT COLOR="#66FFFF"><B>Bite Size (0=ALL):</B></FONT></TD>
        <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=BITE SIZE=5 VALUE="@ItemData?BITE@">
        </TD>
    </TR>
	@if?ItemData?ISSUPERPILL@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Spell Name or Parameters:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=READABLETEXT SIZE=60  MAXLENGTH=255 VALUE="@ItemData?READABLETEXT@">
	    </TD>
	</TR>
	@else@
		@if?ItemData?ISPILL@
		<TR>
			<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Spells to cast on eater:</B></FONT></TD>
		    <TD WIDTH=450 COLSPAN=2>
		    @ItemData?READABLESPELLS@
		    </TD>
		</TR>
		@endif@
	@endif@
@else@
@if?ItemData?ISDRINK@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Liquid Capacity:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=LIQUIDHELD SIZE=5 VALUE="@ItemData?LIQUIDHELD@">
	    </TD>
	</TR>
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Quenched/Drink:</B></FONT></TD>
	    <TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=QUENCHED SIZE=5 VALUE="@ItemData?QUENCHED@">
	    <INPUT TYPE=HIDDEN NAME=ISREADABLE VALUE="">
	    </TD>
	</TR>
	@if?ItemData?ISPOTION@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Spells to cast on drinker:</B></FONT></TD>
		<TD WIDTH=450 COLSPAN=2>
		@ItemData?READABLESPELLS@
		</TD>
	</TR>
	@else@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Liquid:</B></FONT>
	    <TD WIDTH=450 COLSPAN=2>
	    <SELECT NAME=LIQUIDTYPES>
		@ItemData?LIQUIDTYPES@
		</SELECT>
		</TD>
	</TR>
	@endif@
	<TR>
		<TD WIDTH=150><FONT COLOR="#66FFFF"><B>Solid Capacity:</B></FONT></TD>
		<TD WIDTH=450 COLSPAN=2><INPUT TYPE=TEXT NAME=CAPACITY SIZE=5 VALUE="@ItemData?CAPACITY@">
		</TD>
